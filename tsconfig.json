{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "exactOptionalPropertyTypes": true,
    "stripInternal": true,
    "removeComments": false,
  },
  "exclude": [
    "dist"
  ]
}